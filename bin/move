#!/usr/bin/env ruby
require_relative "../lib/move.rb"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]


puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"

usr_input = gets.strip

usr_input.to_i


if (usr_input != nil)

        if is_number?(usr_input)
            input_to_index(usr_input)
        else
          puts "Not a number"
          end


else
 puts "Null input"


end

move(board, index, value)
