#!/usr/bin/env ruby
require_relative "../lib/move.rb"


value = 'X'

print "Welcome to Tic Tac Toe!"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

print "/n"
puts "Where would you like to go?"

usr_input = gets.strip
input_to_index(usr_input)
move(board, usr_input.to_i, value)
display_board(board)
